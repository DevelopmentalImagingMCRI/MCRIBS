#!/bin/bash

export SUBJECTS_DIR=`pwd`/freesurfer

if [ -z "$1" ]
then
	echo "Usage $0 <subjid> [options]"
	exit
fi

ENSUREPIAL=-ensure-pial-is-outside-white-surface
LARGECLOSE=
USEFASTCOLLISION=
JOINTHRESHOLD=1
LARGEVENTRICLES=
NOWMLAPLACIANEXPAND=
DEBUGWHITE=
DEBUGPIAL=
DEBUGCEREBRUM=
CLEANSR=
CLEANPIAL=

while (( "$#" ))
do
	case $1 in
		--noensureoutsidepial)
			ENSUREPIAL=
		;;
		--largeclose)
			LARGECLOSE=YES
		;;
		--fastcollision)
			USEFASTCOLLISION=-use-fast-collision
		;;
		--largeventricles)
			LARGEVENTRICLES=-l
		;;
		--nowmlaplacianexpand)
			NOWMLAPLACIANEXPAND=--nowmlaplacianexpand
		;;
		--cleansr)
			CLEANSR=YES
		;;
		--cleanpial)
			CLEANPIAL=YES
		;;
		--debugcerebrum)
			DEBUGCEREBRUM=--debugcerebrum
		;;
		--debugwhite)
			DEBUGWHITE=--debugwhite
		;;
		--debugpial)
			DEBUGPIAL=--debugpial
		;;
		--jointhreshold)
			JOINTHRESHOLD=$2
		shift;
		;;
		*)
			SUBJID=$1
		;;
	esac
	shift;
done

